<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dialog_corner_bg"
    android:padding="10dp"
    android:orientation="vertical"
    >

    <TextView
        android:id="@+id/myjoin_shop_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawablePadding="5dp"
        android:drawableLeft="@mipmap/dianpu_icon"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:text="北京1号店"
        android:gravity="center_vertical"
        />

    <TextView
        android:id="@+id/myjoin_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawablePadding="5dp"
        android:drawableLeft="@mipmap/location_icon"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:text="北京市朝阳区广顺北大街鹏润大厦117号"
        android:gravity="center_vertical"
        />
    <TextView
        android:id="@+id/myjoin_hehuoren"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawablePadding="5dp"
        android:drawableLeft="@mipmap/hehuoren_icon"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:text="我是A类事业合伙人"
        android:gravity="center_vertical"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/a_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="@color/text_color"
            />
        <TextView
            android:id="@+id/a_type_des"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="#305ba3"
            android:layout_marginTop="5dp"
            />

        <TextView
            android:id="@+id/b_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="@color/text_color"
            android:layout_marginTop="10dp"
            />
        <TextView
            android:id="@+id/b_type_des"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="#305ba3"
            android:layout_marginTop="5dp"
            />

        <TextView
            android:id="@+id/c_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="@color/text_color"
            android:layout_marginTop="10dp"
            />
        <TextView
            android:id="@+id/c_type_des"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="#305ba3"
            android:layout_marginTop="5dp"
            />
        <TextView
            android:id="@+id/d_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="@color/text_color"
            android:layout_marginTop="10dp"
            />
        <TextView
            android:id="@+id/d_type_des"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="#305ba3"
            android:layout_marginTop="5dp"
            />
    </LinearLayout>
    


</com.zhy.autolayout.AutoLinearLayout>
